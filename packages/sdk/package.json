{
  "name": "@arc/sdk",
  "version": "0.0.0",
  "description": "Arc SDK package",
  "main": "./build/sdk.js",
  "license": "MIT",
  "homepage": "https://github.com/interactivetimmy/arc/tree/master/packages/sdk/#readme",
  "repository": "https://github.com/interactivetimmy/arc/packages/sdk",
  "bugs": "https://github.com/interactivetimmy/arc/issues",
  "contributors": [
    "InteractiveTimmy"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "entries": {
    "build": "./build/sdk.js",
    "dist": "./build/index.js",
    "src": "./src/index.js"
  },
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run compile",
    "clean": "rimraf ./build ./dist ./docs",
    "compile": "rollup -c ./rollup.config.js -n SDK -o ./build/sdk.js",
    "docs": "echo \"Error: no compile script defined\" && exit 1",
    "lint": "eslint ./src/** ./test/**",
    "lint:src": "eslint ./src/**",
    "lint:test": "eslint ./test/**",
    "samples": "echo \"Error: no compile script defined\" && exit 1",
    "test": "jest",
    "transpile": "tsc"
  }
}
